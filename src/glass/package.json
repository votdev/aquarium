{
  "name": "glass",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4242",
    "build": "ng build",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint:eslint": "ng lint",
    "lint:html": "htmllint src/app/**/*.html && html-linter --config html-linter.config.json",
    "lint": "run-p -csl --aggregate-output lint:*",
    "fix:prettier": "prettier --write \"{src,e2e}/**/*.{ts,scss}\"",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix": "run-p -csl --aggregate-output fix:*",
    "e2e": "ng e2e",
    "i18n:extract": "ngx-translate-extract --input src --output src/assets/i18n/aquarium.pot --sort --format pot --clean",
    "i18n:pull": "python3 -m venv venv; . venv/bin/activate; pip install transifex-client; tx --root=. pull --all --force",
    "i18n:convert": "run-p -csl --aggregate-output i18n:convert:*",
    "i18n:convert:de_DE": "po2json --format mf --pretty src/assets/i18n/de_DE.po src/assets/i18n/de_DE.json"
  },
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "~/(.*)$": "<rootDir>/src/$1"
    },
    "preset": "jest-preset-angular",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "^11.2.3",
    "@angular/cdk": "^11.2.2",
    "@angular/common": "^11.2.3",
    "@angular/compiler": "^11.2.3",
    "@angular/core": "^11.2.3",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "^11.2.3",
    "@angular/material": "^11.2.2",
    "@angular/platform-browser": "^11.2.3",
    "@angular/platform-browser-dynamic": "^11.2.3",
    "@angular/router": "^11.2.3",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@mdi/angular-material": "5.9.55",
    "@ngx-translate/core": "^13.0.0",
    "@swimlane/ngx-charts": "^17.0.1",
    "dayjs": "^1.10.4",
    "lodash": "^4.17.21",
    "ng-block-ui": "3.0.2",
    "rxjs": "^6.6.6",
    "tslib": "^2.1.0",
    "validator": "^13.5.2",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.0.0",
    "@angular-devkit/build-angular": "0.1101.2",
    "@angular-eslint/builder": "^1.2.0",
    "@angular-eslint/eslint-plugin": "^1.2.0",
    "@angular-eslint/eslint-plugin-template": "^1.2.0",
    "@angular-eslint/schematics": "^1.2.0",
    "@angular-eslint/template-parser": "^1.2.0",
    "@angular/cli": "^11.2.2",
    "@angular/compiler-cli": "^11.2.3",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@types/jasmine": "3.6.3",
    "@types/jest": "25.2.3",
    "@types/lodash": "^4.14.168",
    "@types/node": "12.11.1",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "6.0.0",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "html-linter": "^1.1.1",
    "htmllint-cli": "^0.0.7",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.0",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.1",
    "jest-preset-angular": "8.3.2",
    "npm-run-all": "4.1.5",
    "po2json": "^0.4.5",
    "prettier": "^2.2.1",
    "protractor": "7.0.0",
    "ts-node": "8.3.0",
    "typescript": "~4.1.5"
  }
}
